<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/back_1"
    tools:context=".Questionario">


    <TextView
        android:id="@+id/textQuestionarioID"
        android:layout_width="363dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/textQuestionarioEmail"
        android:layout_centerVertical="true"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/textQuestionarioEmail"
        android:layout_width="363dp"
        android:layout_height="35dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="176dp"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="115dp" />

    <Button
        android:id="@+id/btnQuestionarioLogOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
